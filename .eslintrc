{
  "parser": "babel-eslint",
  "plugins": [ "babel" ],
  "env": { "es6": true, "node": true },
  "ecmaFeatures": { "modules": true },

  "rules": {
    "babel/generator-star-spacing":      [ 2, "both" ],
    "generator-star-spacing":            [ 0 ],
    "babel/object-shorthand":            [ 2, "always" ],
    "object-shorthand":                  [ 0 ],

    "accessor-pairs":
        [ 2, { "setWithoutGet": true } ],
    "brace-style":
        [ 2, "1tbs", { "allowSingleLine": true } ],
    "camelcase":
        [ 2, { "properties": "always" } ],
    "dot-notation":
        [ 2, { "allowKeywords": true, "allowPattern": "^([A-Z]|.*_).*$" } ],
    "indent":
        [ 2, "tab", { "indentSwitchCase": true } ],
    "key-spacing":
        [ 2, { "afterColon": true, "beforeColon": false } ],
    "no-unused-vars":
        [ 2, { "vars": "all", "args": "after-used" } ],
    "one-var":
        [ 2, { "uninitialized": "always", "initialized": "never" } ],
    "semi-spacing":
        [ 2, { "before": false, "after": true } ],
    "space-unary-ops":
        [ 2, { "words": true, "nonwords": false } ],

    "array-bracket-spacing":             [ 2, "always" ],
    "comma-dangle":                      [ 2, "never" ],
    "comma-style":                       [ 2, "last" ],
    "computed-property-spacing":         [ 2, "never" ],
    "consistent-this":                   [ 2, "do_not_alias_this_in_es6" ],
    "curly":                             [ 2, "multi" ],
    "dot-location":                      [ 2, "property" ],
    "func-style":                        [ 2, "expression" ],
    "handle-callback-err":               [ 2, "^err(or)?$" ],
    "linebreak-style":                   [ 2, "unix" ],
    "max-len":                           [ 2, 80, 4 ],
    "no-cond-assign":                    [ 2, "always" ],
    "no-mixed-spaces-and-tabs":          [ 2, "smart-tabs" ],
    "object-curly-spacing":              [ 2, "always" ],
    "operator-assignment":               [ 2, "always" ],
    "operator-linebreak":                [ 2, "after" ],
    "quote-props":                       [ 2, "as-needed" ],
    "quotes":                            [ 2, "single" ],
    "semi":                              [ 2, "always" ],
    "space-after-keywords":              [ 2, "always" ],
    "space-before-blocks":               [ 2, "always" ],
    "space-before-function-paren":       [ 2, "never" ],
    "space-in-parens":                   [ 2, "never" ],
    "spaced-comment":                    [ 2, "always" ],
    "strict":                            [ 2, "never" ],
    "yoda":                              [ 2, "never" ],

    "block-scoped-var":                  [ 0 ],
    "comma-spacing":                     [ 0 ], // Cannot handle elision
    "complexity":                        [ 0 ],
    "consistent-return":                 [ 0 ],
    "constructor-super":                 [ 2 ],
    "default-case":                      [ 0 ],
    "eol-last":                          [ 2 ],
    "eqeqeq":                            [ 0 ],
    "func-names":                        [ 0 ],
    "guard-for-in":                      [ 0 ],
    "lines-around-comment":              [ 0 ],
    "max-depth":                         [ 0 ],
    "max-nested-callbacks":              [ 0 ],
    "max-params":                        [ 0 ],
    "max-statements":                    [ 0 ],
    "new-cap":                           [ 2 ],
    "new-parens":                        [ 2 ],
    "newline-after-var":                 [ 0 ],
    "no-alert":                          [ 0 ],
    "no-array-constructor":              [ 2 ],
    "no-bitwise":                        [ 0 ],
    "no-caller":                         [ 2 ],
    "no-catch-shadow":                   [ 0 ],
    "no-console":                        [ 0 ],
    "no-constant-condition":             [ 0 ],
    "no-continue":                       [ 0 ],
    "no-control-regex":                  [ 0 ],
    "no-debugger":                       [ 2 ],
    "no-delete-var":                     [ 2 ],
    "no-div-regex":                      [ 0 ],
    "no-dupe-args":                      [ 2 ],
    "no-dupe-keys":                      [ 2 ],
    "no-duplicate-case":                 [ 2 ],
    "no-else-return":                    [ 2 ],
    "no-empty":                          [ 2 ],
    "no-empty-character-class":          [ 2 ],
    "no-empty-label":                    [ 0 ],
    "no-eq-null":                        [ 2 ],
    "no-eval":                           [ 2 ],
    "no-ex-assign":                      [ 2 ],
    "no-extend-native":                  [ 2 ],
    "no-extra-bind":                     [ 2 ],
    "no-extra-boolean-cast":             [ 2 ],
    "no-extra-parens":                   [ 0 ],
    "no-extra-semi":                     [ 2 ],
    "no-fallthrough":                    [ 2 ],
    "no-floating-decimal":               [ 2 ],
    "no-func-assign":                    [ 2 ],
    "no-implied-eval":                   [ 2 ],
    "no-inline-comments":                [ 0 ],
    "no-inner-declarations":             [ 2 ],
    "no-invalid-regexp":                 [ 2 ],
    "no-irregular-whitespace":           [ 2 ],
    "no-iterator":                       [ 2 ],
    "no-label-var":                      [ 2 ],
    "no-labels":                         [ 0 ],
    "no-lone-blocks":                    [ 2 ],
    "no-lonely-if":                      [ 2 ],
    "no-loop-func":                      [ 0 ],
    "no-mixed-requires":                 [ 2 ],
    "no-multi-spaces":                   [ 0 ],
    "no-multi-str":                      [ 0 ],
    "no-multiple-empty-lines":           [ 2 ],
    "no-native-reassign":                [ 2 ],
    "no-negated-in-lhs":                 [ 2 ],
    "no-nested-ternary":                 [ 0 ],
    "no-new":                            [ 2 ],
    "no-new-func":                       [ 2 ],
    "no-new-object":                     [ 2 ],
    "no-new-require":                    [ 2 ],
    "no-new-wrappers":                   [ 2 ],
    "no-obj-calls":                      [ 2 ],
    "no-octal":                          [ 2 ],
    "no-octal-escape":                   [ 2 ],
    "no-param-reassign":                 [ 0 ],
    "no-path-concat":                    [ 0 ],
    "no-plusplus":                       [ 0 ],
    "no-process-env":                    [ 0 ],
    "no-process-exit":                   [ 0 ],
    "no-proto":                          [ 2 ],
    "no-redeclare":                      [ 2 ],
    "no-regex-spaces":                   [ 0 ],
    "no-reserved-keys":                  [ 0 ],
    "no-restricted-modules":             [ 0 ],
    "no-return-assign":                  [ 2 ],
    "no-script-url":                     [ 2 ],
    "no-self-compare":                   [ 2 ],
    "no-sequences":                      [ 2 ],
    "no-shadow":                         [ 0 ],
    "no-shadow-restricted-names":        [ 2 ],
    "no-spaced-func":                    [ 2 ],
    "no-sparse-arrays":                  [ 0 ],
    "no-sync":                           [ 2 ],
    "no-ternary":                        [ 0 ],
    "no-this-before-super":              [ 2 ],
    "no-throw-literal":                  [ 2 ],
    "no-trailing-spaces":                [ 2 ],
    "no-undef":                          [ 2 ],
    "no-undef-init":                     [ 2 ],
    "no-undefined":                      [ 0 ],
    "no-underscore-dangle":              [ 2 ],
    "no-unexpected-multiline":           [ 2 ],
    "no-unneeded-ternary":               [ 2 ],
    "no-unreachable":                    [ 2 ],
    "no-unused-expressions":             [ 2 ],
    "no-use-before-define":              [ 0 ], // Canâ€™t enable; TDZ borked.
    "no-var":                            [ 2 ],
    "no-void":                           [ 0 ],
    "no-warning-comments":               [ 2 ],
    "no-with":                           [ 2 ],
    "padded-blocks":                     [ 0 ],
    "prefer-const":                      [ 2 ],
    "radix":                             [ 0 ],
    "sort-vars":                         [ 0 ],
    "space-infix-ops":                   [ 2 ],
    "space-return-throw-case":           [ 2 ],
    "use-isnan":                         [ 2 ],
    "valid-jsdoc":                       [ 0 ],
    "valid-typeof":                      [ 2 ],
    "vars-on-top":                       [ 0 ],
    "wrap-iife":                         [ 0 ],
    "wrap-regex":                        [ 0 ]
  }
}
